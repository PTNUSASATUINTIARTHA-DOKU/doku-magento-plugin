<?xml version="1.0"?>

<body>
    <referenceContainer name="order.success.additional.info">
        <block class="Jokul\Magento2\Block\Checkout\Onepage\Success"
               name="doku.order.success"
               template="Jokul_Magento2::order/success.phtml"
               after="-">
        </block>
    </referenceContainer>
    <referenceBlock name="page.main.title">
            <block class="Jokul\Magento2\Block\Checkout\Onepage\Success" name="checkout.success.print.button" template="button.phtml"/>
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Thank you for your purchase!</argument>
            </action>
        </referenceBlock>
</body>