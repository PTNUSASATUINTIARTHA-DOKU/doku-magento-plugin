<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <doku_general_config>
            <js_merchant_hosted>
                <development_url>https://staging.doku.com/doku-js/assets/js/doku.uncompress-1.4-SNAPSHOOT.js</development_url>
            </js_merchant_hosted>
        </doku_general_config>
        <doku_config>
            <doku_checkout_merchanthosted>
                <title>Jokul Checkout</title>
                <description>Jokul Checkout payment method</description>
            </doku_checkout_merchanthosted>
        </doku_config>
        <payment>
            <doku_checkout_merchanthosted>
                <title>Jokul Checkout</title>
                <description>Bayar pesanan dengan pembayaran melalui Jokul Checkout</description>
                <payment_code_prefix>88899</payment_code_prefix>
                <order_status>pending</order_status>
                <allowspecific>0</allowspecific>
                <group>offline</group>
                <qris_client_id>0</qris_client_id>
                <sharedkey>0</sharedkey>
                <mpan>0</mpan>
                <model>Jokul\Magento2\Model\Payment\CheckOutJokul</model>
            </doku_checkout_merchanthosted>
        </payment>
    </default>
</config>
